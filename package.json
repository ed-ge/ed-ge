{
    "name": "ed-ge",
    "version": "1.0.0",
    "type": "module",
    "description": "The EDucational Game Engine, built with babel.",
    "scripts": {
        "test": "./node_modules/.bin/coverage --reporter=text-summary ./node_modules/.bin/mocha ./test/mocha/**/*.js",
        "cover": "./node_modules/.bin/coverage ./node_modules/.bin/mocha ./test/mocha/**/*.js",
        "rollup": "./node_modules/rollup/dist/bin/rollup src/Base.js --file ./rollup/Base.js --format es --name 'Base'",
        "rollupWatch": "./node_modules/rollup/dist/bin/rollup src/Base.js --file ./rollup/Base.js --format es --name 'Base' -w",
        "free": "rollup src/Base.js --file ./free/Base.js --format iife --name 'Base'",
        "doc": "./node_modules/.bin/jsdoc -d ./docs -r ./src -c ./config/.jsdoc.json"
    },
    "keywords": [
        "Gamengin",
        "Games",
        "Education",
        "unomaha.edu"
    ],
    "author": "B. Ricks, and CSCI 2510 @ unomaha.edu",
    "license": "MIT",
    "devDependencies": {
        "chai": "^4.2.0",
        "chai-string": "^1.5.0",
        "istanbul": "^0.4.5",
        "jsdoc": "^3.6.3",
        "mocha": "^7.1.0",
        "rollup": "^2.7.6",
        "type-detect": "^4.0.8",
        "coverage": "^0.4.1"
    },
    "dependencies": {
        "recursive-readdir": "^2.2.2"
    }
}
